{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.vscode": true,
        "**/main.out.dSYM": true,
        "**/.gitignore": true,
        "**/*.out": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "window.zoomLevel": 0,

    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.leader": "<space>",
    "vim.hlsearch": true,
    "search.location": "panel",

    "editor.fontSize": 14,
}